source ~/base.vimrc
source ~/mapping.vimrc

set surround
set NERDTree
set easymotion
set commentary
set multiple-cursors

" IdeaVim-Extension 切换输入法 "
let context_aware=0
set keep-english-in-normal-and-restore-in-insert

" idea Action "
nnoremap <leader><leader>ren <esc>:action RenameElement<cr><esc><right>
nnoremap <leader><leader>var <esc>:action IntroduceVariable<cr><esc>
