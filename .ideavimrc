source ~/base.vimrc
source ~/mapping.vimrc

set surround
set NERDTree
set easymotion
set commentary
set multiple-cursors

" IdeaVim-Extension 切换输入法 "
let context_aware=0
set keep-english-in-normal-and-restore-in-insert

" idea Action "
map <leader>ren <Action>(RenameElement)<esc>l
map <leader>var <Action>(IntroduceVariable)<esc>
