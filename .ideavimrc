set encoding=utf-8 fileencodings=utf-8
" 系统剪切板 "
set clipboard^=unnamed,unnamedplus
" 相对行号 "
set number relativenumber
" 搜索大小写 "
set ignorecase smartcase 
" 搜索高亮 "
set hlsearch incsearch

set surround
set NERDTree
set easymotion
set commentary
set multiple-cursors
source ~/map.vimrc

" idea Action "
map <leader>ren <Action>(RenameElement)<backspace>
map <leader>var <Action>(IntroduceVariable)<backspace>

